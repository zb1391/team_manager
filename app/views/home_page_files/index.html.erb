<h1>Listing home_page_files</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @home_page_files.each do |home_page_file| %>
      <tr>
        <td><%= home_page_file.name %></td>
        <td><%= link_to 'Show', home_page_file %></td>
        <td><%= link_to 'Edit', edit_home_page_file_path(home_page_file) %></td>
        <td><%= link_to 'Destroy', home_page_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Home page file', new_home_page_file_path %>
